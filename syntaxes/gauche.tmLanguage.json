{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Gauche",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#expression"
		}
	],
	"repository": {
		"expression": {
			"patterns": [
				{
					"include": "#paren-expression"
				},
				{
					"include": "#fileutil"
				},
				{
					"include": "#controlkeywords"
				},
				{
					"include": "#eqkeywords"
				},
				{
					"include": "#listkeywords"
				},
				{
					"include": "#boolkeywords"
				},
				{
					"include": "#define"
				},
				{
					"include": "#constant"
				},
				{
					"include": "#setkeywords"
				},
				{
					"include": "#letkeywords"
				},
				{
					"include": "#libs"
				},
				{
					"include": "#regex"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#math"
				}
			]
		},
		"paren-expression": {
			"begin": "\\(",
			"end": "\\)",
			"beginCaptures": {
				"0": {
					"name": "punctuation.paren.open"
				}
			},
			"endCaptures": {
				"0": {
					"name": "punctuation.paren.close"
				}
			},
			"name": "expression.group",
			"patterns": [
				{
					"include": "#expression"
				}
			]
		},
		"fileutil": {
			"patterns": [
				{
					"name": "keyword.file.util.gauche",
					"match": "\\b(directory-list|file-is-regular\\\\?)\\b"
				}
			]
		},
		"controlkeywords": {
			"patterns": [
				{
					"name": "keyword.control.gauche",
					"match": "\\b(case|cond|cut|do|else|guard|if)\\b"
				}
			]
		},
		"boolkeywords": {
			"patterns": [
				{
					"name": "keyword.logical.gauche",
					"match": "\\b(and|or|not)\\b"
				}
			]
		},
		"eqkeywords": {
			"patterns": [
				{
					"name": "keyword.logical.gauche",
					"match": "\\b(eq|equal|equalv|string=)\\\\?\\b"
				}
			]
		},
		"listkeywords": {
			"patterns": [
				{
					"name": "keyword.list.gauche",
					"match": "\\b(apply|list|car|caar|cdr|cadr|cddr|quote|remove)\\b"
				}
			]
		},
		"define": {
			"patterns": [
				{
					"name": "keyword.function.lisp.scheme.gauche",
					"match": "\\b(define-method|define|lambda)\\b"
				},
				{
					"name": "keyword.type.lisp.scheme.gauche",
					"match": "\\b(define-class)\\b"
				}
			]
		},
		"letkeywords": {
			"patterns": [
				{
					"name": "keyword.let.gauche",
					"match": "\\b(let-values|letrec|let)\\b"
				}
			]
		},
		"setkeywords": {
			"patterns": [
				{
					"name": "keyword.set.gauche",
					"match": "\\b(set\\!|push\\!|pop\\!)\\b"
				}
			]
		},
		"libs": {
			"patterns": [
				{
					"name": "keyword.include.gauche",
					"match": "\\b(include|use)\\b"
				}
			]
		},
		"math": {
			"patterns": [
				{
					"name": "keyword.operator.arithmetic.gauche",
					"match": "\\b(\\-)\\b"
				}
			]
		},
		"constant": {
			"patterns": [
				{
					"name": "constant.language.boolean.lisp.scheme.gauche",
					"match": "#[f|t]"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.gauche",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.gauche",
					"match": "\\\\."
				}
			]
		},
		"regex": {
			"name": "string.regex.gauche",
			"begin": "#/",
			"end": "/",
			"patterns": [
				{
					"name": "constant.character.escape.gauche",
					"match": "\\\\."
				}
			]
		},
		"comment": {
			"name": "comment.line.semicolon.lisp.scheme.gauche",
			"begin": ";",
			"end": "\n"
		}
	},
	"scopeName": "source.lisp.scheme.gauche"
}